<%- include("header"); -%>

<div class="container text-center">
  <div class="row">
    <div class="col-sm-6">
      <div style="text-align: left" class="">
        <p>GALLERY</p>
        <br />
        <% galleryContent.forEach((gallery) => {%>
        <div>
          <P
            ><a href="gallery/<%=gallery._id%>"
              ><img
                src="<%=gallery.image%>"
                style="height: 50px; width: 50px" /></a
          ></P>
        </div>
        <%});%>
      </div>
    </div>
  </div>
</div>
<hr />
<br />

<%- include("footer"); -%>
